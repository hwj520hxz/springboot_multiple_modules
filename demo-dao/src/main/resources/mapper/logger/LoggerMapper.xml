<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hwj.demo.dao.user.LoggerMapper">

    <resultMap id="logMap" type="com.hwj.demo.entity.Logger">
        <id column="id" jdbcType="VARCHAR" property="id" />
        <result column="ip" jdbcType="VARCHAR" property="ip" />
        <result column="operatorId" jdbcType="VARCHAR" property="operatorId" />
        <result column="method" jdbcType="VARCHAR" property="method" />
        <result column="desc" jdbcType="VARCHAR" property="desc" />
    </resultMap>

    <sql id="Base_Column_List">
        id, ip, operatorId, method, desc
    </sql>

    <select id="queryLog" resultMap="logMap">
        select * from user
    </select>

    <insert id="insertLog" parameterType="com.hwj.demo.entity.Logger">
        insert into logger
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="ip != null">
                ip,
            </if>
            <if test="operatorId != null">
                operatorId,
            </if>
            <if test="method != null">
                method,
            </if>
            <if test="description != null">
                description,
            </if>
            <if test="port != null">
                port
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="ip != null">
                #{ip},
            </if>
            <if test="operatorId != null">
                #{operatorId},
            </if>
            <if test="method != null">
                #{method},
            </if>
            <if test="description != null">
                #{description},
            </if>
            <if test="port != null">
                #{port}
            </if>
        </trim>
    </insert>



</mapper>
